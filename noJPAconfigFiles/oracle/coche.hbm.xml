<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.Coche" table="COCHE">

		<!-- Primary Key "Sequence" Oracle-->
		<id name="idCoche" column="ID_COCHE" type="integer">
			<generator class="sequence">
				<param name="sequence_name">SEQ_COCHE</param>
			</generator>		
		</id>
		
		<property name="marca" column="MARCA" type="string"/>
		<property name="modelo" column="MODELO" type="string"/>
		<property name="potencia" column="POTENCIA" type="integer"/>
		
	</class>
</hibernate-mapping>